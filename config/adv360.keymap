	combos {
		cmd_tab_combo {
			timeout-ms = <50>;
			key-positions = <5 6>;// Positions of the T key and the key directly after it
			bindings = <&kp LGUI &kp TAB>;// Command + Tabxx
		};
	};
	
	macros {
	};
	
	keymap {
		compatible = "zmk,keymap";
		base {
			bindings = <
			&kp EQUAL  &pkp_n1          &lkp_n2           &lkp_n3            &lkp_n4               &akp_n5        &tog 1                                                                   &mo 3                 &ekp_n6     &rkp_n7               &rkp_n8             &rkp_n9            &skp_n0              &eprcnt
			&kp TAB    &kp Q            &kp W             &kp E              &kp R                 &kp T          &kp SLASH                                                                &kp NON_US_BACKSLASH  &kp Y      &kp U                  &kp I               &kp O              &kp P                &kp MINUS
			&kp ESC    &hml LGUI A      &hml LEFT_ALT S   &hml LEFT_SHIFT D  &hml LEFT_CONTROL F   &kp G          &col_semi          &kp LEFT_COMMAND  &hml LG(LA(LCTRL)) LCTRL &kp LGUI   &kp RCTRL  &kp SQT               &kp H      &hmr LCTRL J           &hmr RIGHT_SHIFT K  &hmr RIGHT_ALT L   &hmr LGUI SEMICOLON  &kp SINGLE_QUOTE
			&kp LSHFT  &kp Z            &kp X             &kp C              &kp V                 &kp B                                               &kp HOME      &kp PG_UP                                   &kp N      &kp M                  &kp COMMA           &kp DOT            &kp HASH             &kp RSHFT
			&mo 2      &kp GRAVE        &kp CAPS          &kp LEFT           &kp RIGHT                            &back_delete       &hml LS(LGUI) ESC &kp END       &kp PG_DN  &kp ENTER  &kp SPACE                        &kp UP                 &kp DOWN            &kp DOLLAR         &kp AT_SIGN         &mo 2
			>;
		};
		
		keypad {
			bindings = <
			&kp EQUAL  &kp N1     &kp N2    &kp N3    &kp N4     &kp N5  &trans                                                   &mo 3      &kp N6  &kp KP_NUM  &kp KP_EQUAL  &kp KP_DIVIDE  &kp KP_MULTIPLY  &kp MINUS
			&kp TAB    &kp Q      &kp W     &kp E     &kp R      &kp T   &none                                                    &none      &kp Y   &kp KP_N7   &kp KP_N8     &kp KP_N9      &kp KP_MINUS     &kp BSLH
			&kp ESC    &kp A      &kp S     &kp D     &kp F      &kp G   &none     &kp LCTRL  &kp LALT      &kp LGUI   &kp RCTRL  &none      &kp H   &kp KP_N4   &kp KP_N5     &kp KP_N6      &kp KP_PLUS      &kp SQT
			&kp LSHFT  &kp Z      &kp X     &kp C     &kp V      &kp B                        &kp HOME      &kp PG_UP                        &kp N   &kp KP_N1   &kp KP_N2     &kp KP_N3      &kp KP_ENTER     &kp RSHFT

		#include "macros.dtsi"
		#include "version.dtsi"
        #ifndef VERSION_MACRO
		
		macro_ver: macro_ver {
			compatible = "zmk,behavior-macro";
			label = "macro_version";
			#binding-cells = <0>;
			bindings = <&kp RET>;
		};
		
